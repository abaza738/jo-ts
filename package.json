{
  "name": "jo-ts",
  "version": "1.3.0",
  "description": "Discord bot with typescript for Jodanian Aviators Discord.",
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "bot",
    "discord",
    "typescript",
    "aviation",
    "flight sim",
    "poscon",
    "vatsim",
    "flight",
    "simulation",
    "network"
  ],
  "bugs": {
    "url": "https://github.com/abaza738/jo-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abaza738/jo-ts"
  },
  "license": "MIT",
  "author": "Maher Abaza",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "node --loader ts-node/esm/transpile-only --experimental-json-modules src/main.ts",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only --experimental-json-modules src/main.ts",
    "serve": "node --experimental-json-modules build/main.js"
  },
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.11.0",
    "@discordx/importer": "^1.1.10",
    "@discordx/koa": "^1.1.2",
    "@discordx/music": "^4.1.0",
    "@discordx/pagination": "^3.0.0",
    "@discordx/utilities": "^5.0.0",
    "@koa/router": "^12.0.0",
    "axios": "^0.27.2",
    "discord.js": "^14.3.0",
    "discordx": "^11.1.12",
    "dotenv": "^16.0.2",
    "ffmpeg-static": "^5.1.0",
    "isomorphic-unfetch": "^3.1.0",
    "koa": "^2.13.4",
    "libsodium-wrappers": "^0.7.10",
    "moment": "^2.29.4",
    "reflect-metadata": "^0.1.13",
    "spotify-url-info": "^3.1.8",
    "ytsr": "^3.8.0"
  },
  "devDependencies": {
    "@discordx/changelog": "^2.0.11",
    "@types/koa": "^2.13.5",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^18.7.16",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
