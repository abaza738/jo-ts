{
  "name": "jo-ts",
  "version": "1.5.3",
  "description": "Discord bot with typescript for Jodanian Aviators Discord.",
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "bot",
    "discord",
    "typescript",
    "aviation",
    "flight sim",
    "poscon",
    "vatsim",
    "flight",
    "simulation",
    "network"
  ],
  "bugs": {
    "url": "https://github.com/abaza738/jo-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abaza738/jo-ts"
  },
  "license": "MIT",
  "author": "Maher Abaza",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "node --loader ts-node/esm/transpile-only --experimental-json-modules src/main.ts",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only --experimental-json-modules src/main.ts",
    "serve": "node --experimental-json-modules build/main.js",
    "deploy": "rm -r ./build && npm ci && npm run build && (npm run serve > log.log 2> &1 &)"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.14.0",
    "@discordx/importer": "^1.1.10",
    "@discordx/koa": "^1.1.2",
    "@discordx/music": "^4.1.0",
    "@discordx/pagination": "^3.3.1",
    "@discordx/utilities": "^5.1.0",
    "@koa/router": "^12.0.0",
    "axios": "^1.2.1",
    "discord.js": "^14.6.0",
    "discordx": "^11.4.0",
    "dotenv": "^16.0.3",
    "ffmpeg-static": "^5.1.0",
    "isomorphic-unfetch": "^3.1.0",
    "koa": "^2.13.4",
    "libsodium-wrappers": "^0.7.10",
    "moment": "^2.29.4",
    "reflect-metadata": "^0.1.13",
    "spotify-url-info": "^3.1.9",
    "ytsr": "^3.8.0"
  },
  "devDependencies": {
    "@discordx/changelog": "^2.11.5",
    "@types/koa": "^2.13.5",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^18.11.0",
    "@types/node-fetch": "^2.6.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
