{
  "name": "jo-ts",
  "version": "1.0.5",
  "description": "Discord bot with typescript for Jodanian Aviators Discord.",
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "bot",
    "discord",
    "typescript",
    "aviation",
    "flight sim",
    "poscon",
    "vatsim",
    "flight",
    "simulation",
    "network"
  ],
  "bugs": {
    "url": "https://github.com/abaza738/jo-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abaza738/jo-ts"
  },
  "license": "MIT",
  "author": "Maher Abaza",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "node --loader ts-node/esm/transpile-only --experimental-json-modules src/main.ts",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only --experimental-json-modules src/main.ts",
    "serve": "node --experimental-json-modules build/main.js"
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.0",
    "@discordjs/voice": "^0.8.0",
    "@discordx/importer": "^1.1.2",
    "@discordx/koa": "^1.0.6",
    "@discordx/music": "^3.1.9",
    "@discordx/pagination": "^2.0.0",
    "@discordx/utilities": "^4.2.0",
    "@koa/router": "^10.1.1",
    "axios": "^0.26.0",
    "discord.js": "^13.6.0",
    "discordx": "^9.3.0",
    "dotenv": "^16.0.0",
    "ffmpeg-static": "^4.4.1",
    "koa": "^2.13.4",
    "libsodium-wrappers": "^0.7.9",
    "moment": "^2.29.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@discordx/changelog": "^2.0.7",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/node": "^17.0.15",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
